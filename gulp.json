{
    "src": [
        "index.js",
        "src",
        "src/public/js/**/*.js",
        "src/public/pages/**/*.js",
        "src/public/views/*.jade"
    ],
    "dest": "lib",
    "clean": [
        "lib"
    ],
    "tsfiles": [
        "src/**/*.ts"
    ],
    "tsfilesmap": {
        "src/**/*.ts": "lib"
    },
    "resources": "src/**/*.!(ts)",
    "inlineSourcemaps": true
}